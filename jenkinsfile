    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
                echo 'buildddddddddddddddddd...'
               
            }
        }

        stage('Test') {
            steps {
                echo 'Testing...'
                
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying...'
                
            }
        }
    }
}
